{
  "name": "ibero-jet-backend",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "Caramel Point",
  "scripts": {
    "start:env": "docker-compose -f docker-compose.develop.yml up -d",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "pre-commit": "lint-staged && npm run lint",
    "prepare": "husky install",
    "pre-push": "npm run test && npm run test:e2e",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "sort-package-json": "npx sort-package-json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test-staged": "jest --bail --findRelatedTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-staged": "pretty-quick --staged",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/**.controller.ts",
      "**/**.service.ts",
      "**/**.middleware.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "lines": 95
      }
    },
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/passport-jwt": "^3.0.8",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.2",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.21.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
